{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "standard-with-typescript",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "@typescript-eslint/space-before-function-paren": ["error", "never"],
    "@typescript-eslint/comma-dangle": ["error", "only-multiline"]
  }
}
